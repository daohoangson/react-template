{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:coverage": "npm run storybook:test -- --coverage",
    "storybook:kill": "kill $( ps aux | grep .bin/storybook | grep -v grep | awk '{print $2}' )",
    "storybook:pipeline": "sh -c 'npm run storybook -- --ci &' && sleep 5 && npm run storybook:coverage && npm run storybook:kill",
    "storybook:test": "test-storybook",
    "pipeline": "npm run format:check && npm run lint && npm run test:coverage && npm run storybook:pipeline"
  },
  "dependencies": {
    "i18next": "^23.9.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.0.5"
  },
  "devDependencies": {
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-essentials": "^8.0.0-beta.4",
    "@storybook/addon-interactions": "^8.0.0-beta.4",
    "@storybook/addon-links": "^8.0.0-beta.4",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0-beta.4",
    "@storybook/react": "^8.0.0-beta.4",
    "@storybook/react-vite": "^8.0.0-beta.4",
    "@storybook/test": "^8.0.0-beta.4",
    "@storybook/test-runner": "^0.18.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.0",
    "@vitest/ui": "^1.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "storybook": "^8.0.0-beta.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.7",
    "vitest": "^1.3.0"
  }
}
